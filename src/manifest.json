{
  "manifest_version": 2,
  "name": "OTL+",
  "version": "0.0.1",
  "description": "OTL, but less terrible",
  "page_action": {
    "default_title": "OTL+",
    "default_popup": "ui.html"
  },
  "background": {
    "scripts": [ "bootstrap.js" ],
    "persistent": false
  },
  "author": "Audrey Schwarz",
  "content_scripts": [ {
    "matches": [ "https://ebiz.manheim.com/*" ],
    "js": [ "jquery-2.1.0.min.js", "otlInjector.js", "otlInjectorListener.js" ]
  } ],
  "permissions": [
    "tabs",
    "webNavigation"
  ]
}
